<resources>
  <declare-styleable name="GiftView">
    <attr format="dimension" name="width"/>
    <attr format="dimension" name="height"/>
    <attr format="dimension" name="gift_text_size"/>
  </declare-styleable>
  <declare-styleable name="ShapeImageView">
    <attr name="imageType">
      <enum name="none" value="0"/>
      <enum name="cycle" value="1"/>
      <enum name="round" value="2"/>
    </attr>
    <attr format="color" name="borderColor"/>
    <attr format="dimension" name="borderWidth"/>
    <attr format="dimension" name="rectRoundRadius"/>
    <attr name="isFullRound" format="boolean"/>
  </declare-styleable>

  <declare-styleable name="TripButton">
    <attr format="dimension" name="btn_radius"/>
    <attr format="color" name="enable_color"/>
    <attr format="color" name="disable_color"/>
    <attr format="color" name="text_color"/>
    <attr format="string" name="text"/>
    <attr format="dimension" name="text_size"/>
    <attr name="text_style">
      <enum name="normal" value="0"/>
      <enum name="body" value="1"/>
    </attr>
    <attr format="reference" name="btn_background"/>
    <attr format="color" name="stroke_color"/>
    <attr format="dimension" name="stroke_width"/>
  </declare-styleable>

  <declare-styleable name="PageIndexPoint">
    <attr format="color" name="point_normal_color"/>
    <attr format="color" name="point_select_color"/>
    <attr name="point_type">
      <enum name="cycle" value="0"/>
      <enum name="rect" value="1"/>
    </attr>
    <attr format="dimension" name="point_width"/>
    <attr format="dimension" name="point_height"/>
    <attr format="dimension" name="point_padding"/>
  </declare-styleable>

  <declare-styleable name="PullView">
    <attr name="scroll_view">
      <enum name="header" value="0"/>
      <enum name="self" value="1"/>
    </attr>
    <attr format="dimension" name="scroll_max_height"/>
    <attr format="reference" name="header_view_id"/>

    <attr format="boolean" name="resolve_conflict"/>
    <attr format="boolean" name="have_footer_view"/>
    <attr format="boolean" name="have_header_view"/>
    <attr format="boolean" name="have_refresh_view"/>
    <attr format="reference" name="refresh_view_id"/>
    <attr format="boolean" name="have_load_more_view"/>
    <attr format="reference" name="load_more_view_id"/>
  </declare-styleable>

  <declare-styleable name="MapCenterPinView">
    <attr format="dimension" name="pin_width"/>
    <attr format="color" name="pin_color"/>
    <attr format="dimension" name="pin_height"/>
    <attr format="dimension" name="pin_circle_size"/>
    <attr format="color" name="pin_circle_color"/>
    <attr format="color" name="pin_inner_circle_color"/>
  </declare-styleable>

  <declare-styleable name="LoadingView">
    <attr name="loading_type">
      <enum name="point" value="0"/>
      <enum name="progress" value="1"/>
    </attr>
    <attr format="color|reference" name="loading_point_color"/>
    <attr format="dimension" name="loading_point_normal_size"/>
    <attr format="dimension" name="loading_point_selector_size"/>
    <attr format="integer" name="loading_point_count"/>

    <attr name="loading_progress_type">
      <enum name="circle" value="2"/>
      <enum name="line" value="3"/>
    </attr>
    <!--loading line 起始位置-->
    <attr name="loading_line_start">
      <enum name="left" value="0"/>
      <enum name="center" value="1"/>
      <enum name="right" value="2"/>
    </attr>
    <attr format="color|reference" name="loading_progress_color"/>
    <attr name="loading_progressBackgroundColor" format="color"/>
    <attr name="loading_progressStrokeColor" format="color"/>
    <attr name="loading_progressStrokeWidth" format="dimension"/>
    <attr name="loading_progressForegroundColor" format="color"/>
    <attr name="loading_progressRound" format="dimension"/>
    <attr format="dimension|reference" name="loading_progress_width"/>

  </declare-styleable>

  <declare-styleable name="WheelView">
    <attr format="integer" name="itemVisibleNum"/>
    <attr format="color" name="textColorOuter"/>
    <attr format="dimension" name="textSizeOuter"/>
    <attr format="color" name="textColorCenter"/>
    <attr format="dimension" name="textSizeCenter"/>
    <attr format="color" name="lineColor"/>
    <attr format="boolean" name="isLoop"/>
    <attr format="dimension" name="lineSpaceDimens"/>
    <attr format="enum" name="wheelGravity">
      <enum name="left" value="234"/>
      <enum name="center" value="354"/>
      <enum name="right" value="894"/>
    </attr>
  </declare-styleable>

  <declare-styleable name="StarView">
    <attr format="integer" name="starViewNumber"/>
    <attr format="boolean" name="starView_isSlide"/>
    <attr format="dimension" name="starViewWidth"/>
    <attr format="dimension" name="starViewHeight"/>
    <attr format="dimension" name="starViewRightMargin"/>
  </declare-styleable>

  <declare-styleable name="ScriptTextView">
    <attr format="color" name="script_color"/>
    <attr format="reference" name="script_text"/>
    <attr format="dimension" name="script_text_size"/>
    <attr format="enum" name="script_direction">
      <enum name="right" value="0"/>
      <enum name="left" value="1"/>
    </attr>
    <attr format="enum" name="script_loc">
      <enum name="top" value="0"/>
      <enum name="mid" value="1"/>
      <enum name="bottom" value="2"/>
    </attr>
    <attr format="dimension" name="script_width"/>
  </declare-styleable>

  <!-- 自定义验证码输入框-->
  <declare-styleable name="verificationCodeView">
    <!--输入框的数量-->
    <attr format="integer" name="vcv_et_number"/>
    <!--输入类型-->
    <attr name="vcv_et_inputType">
      <enum name="number" value="0"/>
      <enum name="numberPassword" value="1"/>
      <enum name="text" value="2"/>
      <enum name="textPassword" value="3"/>
    </attr>
    <!--输入框的宽度-->
    <attr format="dimension|reference" name="vcv_et_width"/>
    <!--输入框文字颜色-->
    <attr format="color|reference" name="vcv_et_text_color"/>
    <!--输入框文字大小-->
    <attr format="dimension|reference" name="vcv_et_text_size"/>
    <!--输入框背景-->
    <attr format="reference" name="vcv_et_bg"/>
    <!--光标样式-->
    <attr format="reference" name="vcv_et_cursor"/>
  </declare-styleable>

  <declare-styleable name="XEditText">
    <attr format="string" name="x_separator"/>
    <attr format="boolean" name="x_disableClear"/>
    <attr format="reference" name="x_clearDrawable"/>
    <attr format="reference" name="x_showPwdDrawable"/>
    <attr format="reference" name="x_hidePwdDrawable"/>
    <attr format="boolean" name="x_disableEmoji"/>
    <attr format="boolean" name="x_togglePwdDrawableEnable"/>
    <attr format="string" name="x_pattern"/>
  </declare-styleable>

  <declare-styleable name="ProgressView">
    <attr format="integer" name="progress_from"/>
    <attr format="integer" name="progress_to"/>
    <attr format="dimension" name="progress_width"/>
    <attr format="color" name="progress_bg"/>
    <attr format="dimension" name="progress_radius"/>
    <attr format="reference" name="progress_foreground"/>
    <attr format="integer" name="progress_max_tick"/>
  </declare-styleable>

  <declare-styleable name="BlurLayout">
    <attr format="float" name="blk_downscaleFactor"/>
    <attr format="integer" name="blk_blurRadius"/>
    <attr format="integer" name="blk_fps"/>
    <attr format="dimension" name="blk_cornerRadius"/>
  </declare-styleable>

  <declare-styleable name="OptionsView">
    <attr name="options_type">
      <enum name="rect" value="0"/>
      <enum name="oval" value="1"/>
    </attr>
    <attr format="reference" name="options_op"/>
    <attr format="boolean" name="options_toggle"/>
    <attr format="color" name="options_toggle_on_bg"/>
    <attr format="color" name="options_toggle_off_bg"/>
    <attr format="dimension" name="options_min_height"/>
    <attr format="dimension" name="options_min_width"/>
    <attr format="dimension" name="options_bg_outer_radius"/>
    <attr format="dimension" name="options_bg_inner_radius"/>
    <attr format="color" name="options_background_color"/>
    <attr format="color" name="options_foreground_color"/>
    <attr format="color" name="options_text_default_color"/>
    <attr format="color" name="options_text_selected_color"/>
    <attr format="dimension" name="options_text_size"/>
    <attr format="string" name="options_op_key"/>
    <attr format="integer" name="options_default_select"/>
  </declare-styleable>

  <declare-styleable name="ListItemWithRightArrowLayout">
    <attr format="reference|string" name="itemTitle"/>
    <attr format="color|reference" name="itemTitleColor"/>
    <attr format="dimension|reference" name="itemMargin"/>
    <attr format="dimension|reference" name="itemTitleSize"/>
    <attr format="boolean" name="itemShowArrow"/>
    <attr format="string" name="itemRightTitle"/>
    <attr format="color|reference" name="itemRightTitleColor"/>
    <attr format="reference" name="itemRightUnit"/>
    <attr format="color" name="itemRightUnitColor"/>
    <attr format="integer" name="itemRightUnitSize"/>
  </declare-styleable>

</resources>